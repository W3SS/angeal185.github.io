    way.set("OSRightData", {{skills.OSRightData | dump | safe}});
    way.set("otherLeftData", {{skills.otherLeftData | dump | safe}});
    way.set("otherRightData", {{skills.otherRightData | dump | safe}});
    way.set("APILeftData", {{skills.APILeftData | dump | safe}});
    way.set("APIRightData", {{skills.APIRightData | dump | safe}});
</script>
