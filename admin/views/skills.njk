        <div class="col s12 m6">
            <h3>JSON output</h3>
            <pre way-data="__all__" way-json="true" way-default="{}" class=""></pre>
            <div class="col s6">
                <button class="btn btn-default" way-clear>Clear data</button>
            </div>
            <form role="form" action="/update{{title}}" method="post">
                <div class="col s6">
                    <button type="submit" class="btn btn-default right">Save changes</button>
                </div>
