            <h3>{{title}} list</h3>
            <form role="form" way-data="codeLeftData">
                <h5>Skills left:{{skills.codeLeftData | length}}</h5>
                <div class="form-group"><a href="#" way-action-push="codeLeftData">Add entry</a></div>
                <div class="form-group" way-repeat="codeLeftData">
                    <div>
                      <div class="item-entry">
                          {% for item in config.skillsData %}
                          <label>{{item}}</label>
                          <input type="text" class="form-control" placeholder="{{item}}" way-data="{{item}}">
                          {% endfor %}
                        </div>
        						<a href="#" way-action-remove="codeLeftData.$$key">Remove entry</a>
        					</div>
                </div>
            </form>
