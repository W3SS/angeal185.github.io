{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
    "js": "node server.js"
  },
  "watch": [
    "index.html",
    "nodemon.json",
    "static/",
	"views/",
	"models/",
	"controllers/"
  ],
  "ext": "js, json, html, css, tpl"
}
